{
  "spaces": 2,
  "generator-cli": {
    "version": "7.2.0",
    "generators": {
      "client": {
        "generatorName": "typescript-axios",
        "output": "./src",
        "glob": "openapi.yaml",
        "templateDir": "templates-js",
        "additionalProperties": {
          "modelPropertyNaming": "original",
          "useSingleRequestParameter": "true",
          "withSeparateModelsAndApi": "true",
          "apiPackage": "api",
          "modelPackage": "models"
        },
        "typeMappings": {
          "set": "Array"
        }
      }
    }
  }
}
